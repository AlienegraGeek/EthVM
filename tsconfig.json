{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "lib": ["dom", "es2015", "es5"],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "paths": {
      "@/*": ["*"]
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": false,
    "target": "es5"
  }
}
