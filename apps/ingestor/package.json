{
  "name": "ethereum-processor",
  "version": "0.0.1",
  "description": "Processor to process Ethereum balance data",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "start:erc20": "nodemon --exec babel-node src/post-processors/erc20/index.js",
    "start:forever": "nodemon --exec 'babel-node src/index.js || touch src/index.js'",
    "start:erc20:forever": "nodemon --exec 'babel-node src/post-processors/erc20/index.js || touch src/post-processors/erc20/index.js'",
    "build": "babel src -d dist",
    "prettier": "prettier --write 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyEtherWallet/ethereum-processor.git"
  },
  "author": "kvhnuke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MyEtherWallet/ethereum-processor/issues"
  },
  "homepage": "https://github.com/MyEtherWallet/ethereum-processor#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "aws-sdk": "^2.514.0",
    "bn.js": "^5.0.0",
    "cli-progress": "^3.0.0",
    "dotenv": "^8.1.0",
    "ethereumjs-common": "^1.3.1",
    "ethereumjs-util": "^6.1.0",
    "level": "^5.0.1",
    "lzutf8": "^0.5.5",
    "mongodb": "^3.3.0",
    "request": "^2.88.0",
    "rlp": "^2.2.3",
    "rpc-websockets": "^4.6.1",
    "web3": "^1.2.1",
    "ws": "^7.1.2"
  }
