{
  "name": "ethvm-api",
  "version": "0.1.0",
  "description": "Backend API for EthVM",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enKryptIO/ethvm.git"
  },
  "keywords": [
    "ethvm",
    "socketio",
    "node",
    "socket",
    "ethereum"
  ],
  "author": "EnKryptIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enKryptIO/ethvm-server/issues"
  },
  "homepage": "https://github.com/enKryptIO/ethvm-server#readme",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "nodemon",
    "format": "yarn lint:fix && yarn prettier",
    "lint": "tslint -c tslint.json --project .",
    "lint:fix": "tslint -c tslint.json --project . --fix",
    "test:unit": "jest --config jest.unit.json",
    "test:e2e": "jest --forceExit --config ./test/jest.e2e.json",
    "test:cov": "jest --coverage --config jest.unit.json",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "dependencies": {
    "@enkrypt.io/ethereumjs-vm": "^2.3.3",
    "@types/jest": "^23.3.5",
    "ajv": "^6.5.4",
    "ajv-keywords": "^3.2.0",
    "bignumber.js": "^8.0.1",
    "convict": "^4.4.0",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-account": "^2.0.5",
    "ethvm-common": "file:./../common",
    "eventemitter3": "^3.1.0",
    "ioredis": "^4.2.0",
    "jayson": "^2.1.0",
    "lodash": "^4.17.11",
    "lru-cache": "^4.1.3",
    "merkle-patricia-tree": "^2.3.2",
    "mongodb": "^3.1.8",
    "node-fetch": "^2.2.0",
    "pino": "^5.10.0",
    "pino-pretty": "^2.5.0",
    "socket.io": "^2.1.1",
    "ts-interface-checker": "^0.1.4",
    "web3-provider-engine": "^14.1.0",
    "web3-utils": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/ioredis": "^4.0.4",
    "@types/json-schema": "^7.0.1",
    "@types/lodash": "^4.14.117",
    "@types/lru-cache": "^4.1.1",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.12",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.2",
    "@types/pino": "^5.6.1",
    "@types/redis": "^2.8.7",
    "@types/socket.io": "^2.1.0",
    "chai": "^4.2.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-environment-node": "^23.4.0",
    "mocha": "^5.2.0",
    "nodemon": "1.18.7",
    "prettier": "1.15.2",
    "shelljs": "^0.8.2",
    "socket.io-client": "^2.1.1",
    "ts-jest": "^23.10.4",
    "ts-mockito": "^2.3.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-consistent-codestyle": "^1.13.3",
    "typescript": "^3.1.3"
  }
}
