{
  "name": "postgres-deltas-sink",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": 1,
    "max.poll.records": 1000,
    "topics": "fungible_balance_deltas,non_fungible_balance_deltas",
    "insert.mode": "insert",
    "pk.mode": "none",
    "connection.url": "jdbc:postgresql://timescale:5432/ethvm_dev",
    "connection.user": "postgres",
    "connection.password": "1234",
    "transforms": "snakeCaseKey,snakeCaseValue,toJsonValue",
    "transforms.snakeCaseKey.type": "com.ethvm.kafka.connect.sources.jdbc.SnakeCase$Key",
    "transforms.snakeCaseValue.type": "com.ethvm.kafka.connect.sources.jdbc.SnakeCase$Value",
    "transforms.toJsonValue.type": "com.ethvm.kafka.connect.sources.jdbc.JsonField$Value",
    "transforms.toJsonValue.whitelist": "trace_location"
  }
}
